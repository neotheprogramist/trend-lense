{
  "name": "trendlens_frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm i && dfx canister create trendlens_backend && dfx generate trendlens_backend && dfx deploy",
    "start": "vite --port 3000",
    "prebuild": "dfx generate",
    "build": "tsc && vite build",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\"",
    "test": "vitest",
    "lint": "prettier --check . && eslint."
  },
  "dependencies": {
    "@dfinity/agent": "^1.4",
    "@dfinity/auth-client": "^1.4.0",
    "@dfinity/candid": "^1.4",
    "@dfinity/identity": "^1.4",
    "@dfinity/principal": "^1.4"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@types/eslint": "^8.56.0",
    "@types/node": "^20.14.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0-next.4",
    "postcss": "^8.4.38",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.2.3",
    "sass": "^1.77.2",
    "svelte": "^5.0.0-next.1",
    "svelte-check": "^3.7.1",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-environment": "^1.1.3",
    "vitest": "^1.6.0"
  }
}
